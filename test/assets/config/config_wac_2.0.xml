         version="2.0"
         height="684"
         width="480">
    <icon src="Icon.png"/>
    <icon src="icon.png"/>
    <content src="index.html"/>
    <preference name = "pref-readwrite"
               value = "readwrite"/>
    <preference name = "pref-readonly"
               value = "readonly"
            readonly = "true" />
    <feature name="http://wacapps.net/api/accelerometer">
        <param name="accuracy" value="low"/>
    </feature>
    <feature name="http://wacapps.net/api/orientation"/>
    <feature name="http://wacapps.net/api/deviceapis"/>
    <feature name="http://wacapps.net/api/camera.show"/>
    <name short="demoWidget" xml:lang="jp">
        <span xml:lang="en-us">Ripple Demo Widget(us)</span>
    </name>
    <name short="demoWidget">
        <span xml:lang="en-uk">Ripple Demo Widget(uk)</span>
    </name>
    <description>The Ripple Emulator Demo Widget</description>
